{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/adminimal_admin_toolbar": "^1.9",
        "drupal/adminimal_theme": "^1.5",
        "drupal/commerce_shipping": "^2.0",
        "drupal/console": "^1.9",
        "drupal/core": "^8.7",
        "drupal/devel": "^2.0",
        "drupal/entity_reference_revisions": "^1.6",
        "drupal/filefield_sources": "^1.0",
        "drupal/image_widget_crop": "^2.2",
	"drupal/imagefield_tokens": "^2.12",
        "drupal/material_admin": "^1.0",
        "drupal/metatag": "^1.0",
        "drupal/mixitup_views": "^1.0",
        "drupal/module_filter": "^3.1",
        "drupal/multiple_registration": "^2.6",
        "drupal/paragraphs": "^1.6",
        "drupal/profile": "^1.0@RC",
        "drupal/range": "^1.1",
        "drupal/restui": "^1.16",
	"drupal/recaptcha": "^2.4",
        "drupal/schema_metatag": "^1.0",
        "drupal/swiftmailer": "^1.0",
        "drupal/ultimate_cron": "^2.0",
	"drupal/webform": "^5.4",
        "drush/drush": "^9.0.0",
        "guzzlehttp/guzzle": "^6.3",
        "phpunit/phpunit": "^6.5",
        "symfony/debug": "~3.0",
        "symfony/dom-crawler": "~3.0",
        "symfony/filesystem": "~3.0",
        "symfony/finder": "~3.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    },
    "require-dev": {
        "behat/mink": "^1.7",
        "behat/mink-goutte-driver": "^1.2",
        "symfony/phpunit-bridge": "^4.3"
    }
}
